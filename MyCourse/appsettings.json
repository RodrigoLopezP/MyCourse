{
     "Logging":{
          "LogLevel":{
               "Default":"Warning"
          }
     },
     "ConnectionStrings": {
          "Default":"DataSource=data/MyCourse.db"
     },

     "Courses":{
          "PerPage": 10,
          "Order": {
               "By": "Rating",
               "Ascending" : true,
               "Allow" :["Title","Rating","CurrentPrice"]
          }
     },
     "MemoryCache":{
          "SizeLimit": 1000
     },
     "ResponseCache":{
          "Home":{
               "Duration" : 60,
               "Location" : "Client",
               "VaryByQueryKeys": ["page"]
          }
     }
} 